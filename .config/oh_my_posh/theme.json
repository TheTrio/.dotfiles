{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "horizontal_offset": 0,
            "vertical_offset": 0,
            "segments": [
                {
                    "type": "session",
                    "style": "diamond",
                    "foreground": "#fff",
                    "background": "#3a0ca3",
                    "powerline_symbol": "",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "display_host": false
                    }
                },
                {
                    "background": "#feae34",
                    "foreground": "#262b44",
                    "powerline_symbol": "",
                    "properties": {
                        "style": "folder"
                    },
                    "style": "powerline",
                    "type": "path"
                },
                {
                    "background": "#fee761",
                    "foreground": "#262b44",
                    "properties": {
                        "display_stash_count": true,
                        "display_upstream_icon": true,
                        "status_colors_enabled": true,
                        "local_changes_color": "#f77622",
                        "ahead_and_behind_color": "#e43b44",
                        "behind_color": "#f77622",
                        "ahead_color": "#2ce8f5",
                        "stash_count_icon": "\uF692 "
                    },
                    "style": "powerline",
                    "type": "git"
                },
                {
                    "background": "#fee761",
                    "foreground": "#262b44",
                    "powerline_symbol": "",
                    "style": "powerline",
                    "type": "root"
                },
                {
                    "background": "#ff0044",
                    "foreground": "#ffffff",
                    "leading_diamond": "<transparent, #ff0044></>",
                    "properties": {
                        "always_enabled": false,
                        "color_background": true,
                        "display_exit_code": true,
                        "error_color": "#ff0044"
                    },
                    "style": "diamond",
                    "trailing_diamond": "",
                    "type": "exit"
                }
            ],
            "type": "prompt"
        },
        {
            "type": "prompt",
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "type": "text",
                    "style": "plain",
                    "foreground": "#007ACC",
                    "properties": {
                        "prefix": "",
                        "text": "\u276F"
                    }
                }
            ]
        }
    ],
    "final_space": true
}
