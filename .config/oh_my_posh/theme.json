{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "horizontal_offset": 0,
      "vertical_offset": 0,
      "segments": [
        {
          "type": "session",
          "style": "diamond",
          "foreground": "#000",
          "background": "#a100f2",
          "powerline_symbol": "",
          "leading_diamond": " <transparent, #a100f2></>",
          "trailing_diamond": "",
          "properties": {
            "display_host": false
          }
        },
        {
          "type": "python",
          "style": "diamond",
          "powerline_symbol": "\uE0B0",
          "foreground": "#100e23",
          "background": "#906cff",
          "properties": {
            "prefix": " \uE235 "
          },
          "leading_diamond": "<transparent, #906cff></>",
          "trailing_diamond": ""
        },
        {
          "type": "node",
          "style": "diamond",
          "powerline_symbol": "\uE0B0",
          "foreground": "#000",
          "background": "#6CA35E",
          "properties": {
            "prefix": " \uE718 ",
            "display_package_manager": true
          },
          "leading_diamond": "<transparent, #6CA35E></>",
          "trailing_diamond": ""
        },
        {
          "background": "#feae34",
          "foreground": "#262b44",
          "powerline_symbol": "",
          "properties": {
            "style": "folder"
          },
          "style": "powerline",
          "type": "path"
        },
        {
          "background": "#fee761",
          "foreground": "#262b44",
          "properties": {
            "display_stash_count": true,
            "display_upstream_icon": true,
            "status_colors_enabled": true,
            "local_changes_color": "#f77622",
            "ahead_and_behind_color": "#e43b44",
            "behind_color": "#f77622",
            "ahead_color": "#2ce8f5",
            "display_status": true,
            "stash_count_icon": "\uF692 "
          },
          "style": "diamond",
          "trailing_diamond": "",
          "type": "git"
        },
        {
          "background": "#fee761",
          "foreground": "#262b44",
          "powerline_symbol": "",
          "style": "powerline",
          "type": "root"
        }
      ],
      "type": "prompt"
    },
    {
      "type": "prompt",
      "alignment": "right",
      "newline": false,
      "segments": [
        {
          "type": "executiontime",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#ffffff",
          "background": "#8800dd",
          "properties": {
            "always_enabled": true,
            "style": "austin",
            "prefix": " <#fefefe>\ufbab</> "
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#007ACC",
          "properties": {
            "prefix": "",
            "text": "\u276F"
          }
        }
      ]
    }
  ],
  "final_space": true
}
